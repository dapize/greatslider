doctype html
html(lang="es")
	head
		title Greade Slider
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width, initial-scale=1, maximum-scale=1")
		link(rel="stylesheet", href="css/styles.css")
		// estilos solo de muestra
		style.
			#breakpoints, #bpsimples {
				margin-left:auto;
				margin-right:auto;
			}

			#bpsimples {
				max-width: 1024px;
			}

			.gs-container-slider {
				max-width: 100%;
			}

			.gs-wrapper-content img {
				display :block;
			}

			/*
			.gs-wrapper-content img {
				margin-left:auto;
				margin-right:auto;
				max-width: 100%;
				max-height: 100%;
				display:block;
			}
			*/

			h2 {
				text-align:center
			}

			.botones button {
				padding:10px;
				border:1px solid #c0c0c0;
				border-radius: 2px;
				background-color: #e7e7e7;
				transition: all .1s linear;
				font-size: 15px;
			}

			.botones button:hover {
				cursor:pointer
			}

			.botones button:active {
				background-color: #555555;
				border-color:#555555;
				color: #fff;
			}

			#go {
				margin-right: 20px
			}

			#prev, #stop {
				margin-right: 10px
			}

			#play {
				margin-right: 2px
			}

			#active, #next {
				margin-right: 5px
			}

			.botones {
				text-align:center;
				margin-top:20px;
			}

			/* Estilos para los testimonios */
			.lostestimonios {
				width: 100%;
				max-width: 700px;
				padding: 20px;
				margin-right: auto;
				margin-left: auto;
			}

			.lostestimonios .gs-container-navs {
				margin-top: 10px;
			}

			.lostestimonios .gs-container-items {
				background-color: #fff;
			}

			.lostestimonios .gs-wrapper-bullets {
				display:flex;
				justify-content: center;
				align-items: center;
			}

			.lostestimonios .gs-wrapper-bullets .gs-bullet {
				width: 15px;
				height: 15px;
				background-color: #c0c0c0;
				border-radius: 50%;
				margin-right: 7px;
			}

			.lostestimonios .gs-wrapper-bullets .gs-bullet:hover {
				cursor: pointer;
			}

			.lostestimonios .gs-wrapper-bullets .gs-bullet.gs-bullet-active {
				background-color: #DFAA57;
			}

			.lostestimonios .gs-wrapper-bullets .gs-bullet:last-child {
				margin-right: 0;
			}

			.testimonio {
				padding:0 40px;
				position: relative;
			}

			.testimonio:before, .testimonio:after {
				content: "";
				width: 22px;
				height: 22px;
				background-image:url('../img/comilla.png');
				background-repeat: no-repeat;
				position: absolute;
			}

			.testimonio:before {
				top:0;
				left:0;
			}

			.testimonio:after {
				bottom:22px;
				right: 0;
				transform: rotate(-180deg)
			}

			.testimonio p {
				text-align: justify;
				line-height: 1.5;
				font-family: "calibri";
				font-size: 15px;
				margin-top: 0;
				margin-bottom: 10px;
				text-align-last: center;
			}

			.testimonio p:last-child {
				margin-bottom: 0;
			}

			.autor {
				margin-top: 15px;
				display: block;
				text-align: left;
				font-weight: bold;
				color: #DFAA57;
			}

			/* Slider de fotos */
			#fotos, #fotoswipe, #fotosb {	
				max-width: 1000px;
				margin-left: auto;
				margin-right: auto;
			}

			.img-slider {
				width: 100%;
			}

			/* Slider de fotos no uniformes */

			#fotosc {
				max-width: 1000px;
				margin-left: auto;
				margin-right: auto;
			}

			#videos, #fotosdata {
				max-width: 850px;
				margin-left: auto;
				margin-right: auto;
			}

			#videos video, #videos iframe{
				width: 100%;
			}

			#videos .gs-wrapper-content {
				width: 100%;
			}

			#fotosdata {
				max-width: 1200px;
			}

			#fsBtn {
				margin-top: 15px;
				margin-left: auto;
				margin-right: auto;
				display:block;
				width: 100px;
				height: 45px;
			}

